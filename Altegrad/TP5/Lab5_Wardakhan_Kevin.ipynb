        "from transformers import AutoModelForCausalLM, AutoTokenizer\n",
        "from transformers import get_linear_schedule_with_warmup\n",
        "from tqdm.auto import tqdm\n",
        "import torch\n",
        "from torch.utils.data import DataLoader\n",
        "import numpy as np\n",
        "\n",
        "device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n",
        "\n",
        "student_model = AutoModelForCausalLM.from_pretrained(\n",
        "    \"Qwen/Qwen2.5-0.5B-Instruct\"\n",
        ").to(device) # To Complete\n",
        "dataset = QwenKLDataset(\"logprobs.h5\", \"conversations_rag.jsonl\") # To Complete\n",
        "\n",
        "\n",
        "num_epochs = 2\n",
        "grad_accum_steps = 8\n",
        "optimizer = torch.optim.AdamW(student_model.parameters(), lr=5e-5)\n",
        "loader = DataLoader(dataset, batch_size=1, shuffle=True, collate_fn=lambda x: x[0])\n",
        "num_training_steps = num_epochs * len(loader) // grad_accum_steps\n",
        "scheduler = get_linear_schedule_with_warmup(optimizer, 0, num_training_steps)\n",
        "loss_fn = torch.nn.KLDivLoss(reduction = \"batchmean\", log_target = True) # To Complete\n",
        "\n",
        "for epoch in range(num_epochs):\n",
        "    for i, batch in enumerate(loader):\n",
        "        input_ids = batch[\"inputs\"][\"input_ids\"].to(device)\n",
        "        outputs = student_model(input_ids)        # To Complete\n",
        "        logits = outputs.logits  # [B, T, V]\n",
        "\n",
        "\n",
        "        start = batch[\"idx_subseq\"]\n",
        "        end = start + batch[\"seq_len\"]\n",
        "        student_logits_ans = logits[0, start:end, :]              # [L, V]\n",
        "        student_log_probs = torch.log_softmax(student_logits_ans, dim=-1)\n",
        "\n",
        "        teacher_log_probs = batch[\"labels\"].to(device)            # [L, V]\n",
        "\n",
        "        batch_loss = loss_fn(student_log_probs, teacher_log_probs)\n",
        "        batch_loss = batch_loss / grad_accum_steps\n",
        "        batch_loss.backward()\n",
        "        if (i + 1) % grad_accum_steps == 0 or (i + 1) == len(loader):\n",
        "            optimizer.step()\n",
        "            scheduler.step()\n",
        "            optimizer.zero_grad()\n",
        "            print (f\"Epoch {epoch+1}, step {i+1}/{len(loader)}: loss = {batch_loss.item():.4f}\")\n",
        "    print(f\"Epoch {epoch+1}: loss = {batch_loss.item():.4f}\")\n",
        "\n",
        "student_model.save_pretrained(\"finetuned_model\")\n",
        "tokenizer.save_pretrained(\"finetuned_model\")\n"
      ]
